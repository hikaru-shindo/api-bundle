{
    "name": "saikootau/api-bundle",
    "description": "A symfony bundle for simple apis.",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.2.0",
        "jms/serializer": "^1.11",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/http-kernel": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Saikootau\\ApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saikootau\\ApiBundle\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Nitsche",
            "email": "saikootau@gmail.com"
        }
    ]
}
